#include<stdio.h>
#include<conio.h>
#include<math.h>
void main()
{
    int n,s=0,r;
    printf("enter any number");
    scanf("%d" ,&n);
    while(n!=0)
    {
        r=n%10;
        s=s+pow(r,3);
        n=n/10;
    }
    if (n==s)
    {
        printf("armstrong");
    }
    else
    {
        printf("not armstrong");
    }
getch();
}
