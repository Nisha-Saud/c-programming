#include<stdio.h>
int main()
{
    int s=0,n,r,a; 
    printf("enter any number");
    scanf("%d",&n);
    a=n;
    while(n!=0)
    {
        r=n%10;
        s=s*10+r;
        n=n/10;
    }
    if(a==s)
    {
        printf("palindrome");
    }
    else
    {
        printf("not palindrome");
    }
    return 0;
}
